{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc"]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "useLongnameInNav": true
    }
  },
  "opts": {
    "destination": "./doc/api/",
    "encoding": "utf8",
    "private": true,
    "template": "node_modules/minami"
  }
}
